# App mode: debug or release
APP_MODE=debug

# Database configuration
# Database URL: postgres://username:password@host:port/database?sslmode=disable
DATABASE_URL=postgres://postgres:postgres@localhost:5432/web?sslmode=disable
DATABASE_POOL_SIZE=10

# Frontend
FRONTEND_URL=http://localhost:3000

# App host and port
APP_HOST=127.0.0.1
APP_PORT=8080

# Jwt secret key and refresh key
JWT_SECRET_
JWT_REFRESH_

# Jwt token expiration time in seconds (300 = 5 minutes)
JWT_EXPIRATION_SECONDS=300

# Jwt refresh token expiration time in seconds (604800 = 7 days)
JWT_REFRESH_SECONDS=604800

# SMTP config
SMTP_HOST=127.0.0.1
SMTP_PORT=25
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=no-reply@pillarsoft.com
